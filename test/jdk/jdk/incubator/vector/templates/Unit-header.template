    static void assertArraysEqualsAssociative($type$[] rl, $type$[] rr, $type$[] a, $type$[] b, $type$[] c, FBinOp f) {
        int i = 0;
        try {
            for (; i < a.length; i++) {
                //Left associative
                AssertEquals(rl[i], f.apply(f.apply(a[i], b[i]), c[i]));

                //Right associative
                AssertEquals(rr[i], f.apply(a[i], f.apply(b[i], c[i])));

                //Results equal sanity check
                AssertEquals(rl[i], rr[i]);
