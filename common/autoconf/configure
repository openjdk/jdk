fi


if test "x$enable_openjdk_only" = "xyes"; then
    OPENJDK=true
elif test "x$enable_openjdk_only" = "xno"; then
    OPENJDK=false
elif test -d "$SRC_ROOT/jdk/src/closed"; then
    OPENJDK=false
