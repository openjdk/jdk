        <!-- luminous-intensity -->
        <convertUnit source='candela' baseUnit='candela' systems="si metric prefixable"/>
        
        <!-- mass -->
        <convertUnit source='dalton' baseUnit='kilogram' factor='AMU' systems="si_acceptable metric prefixable"/>
