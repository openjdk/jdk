<ldml>
	<identity>
		<version number="$Revision$"/>
		<language type="raj"/>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="en">अंग्रेजी</language>
			<language type="raj">राजस्थानी</language>
		</languages>
		<scripts>
			<script type="Deva">देवनागरी</script>
		</scripts>
		<territories>
			<territory type="IN">भारत</territory>
			<territory type="ZZ">अज्ञात</territory>
		</territories>
		<measurementSystemNames>
			<measurementSystemName type="metric">मीट्रिक</measurementSystemName>
			<measurementSystemName type="UK">यूके</measurementSystemName>
			<measurementSystemName type="US">यू एस</measurementSystemName>
		</measurementSystemNames>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters draft="contributed">[ँ ंः अ क ख ग घ ङ च छ ज झ ञ ट ठ ड ढ ण त थ द ध न प फ ब भ म य र ल व श ष स ह ा ि ी ु ू ृ ॅ े ै ो ौ ्]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[आ इ ई उ ऊ ऋ ए ऐ ओ औ]</exemplarCharacters>
		<exemplarCharacters type="numbers" draft="contributed">[\- ‑ , . % ‰ + 0० 1१ 2२ 3३ 4४ 5५ 6६ 7७ 8८ 9९]</exemplarCharacters>
		<exemplarCharacters type="punctuation" draft="contributed">[\- ‑ , \: ! ? . … '‘’ &quot;“” ( ) \[ \] \{ \} § @ * / \\ \&amp; # ` |]</exemplarCharacters>
	</characters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="wide">
							<month type="1">जनवरी</month>
							<month type="2">फरवरी</month>
							<month type="3">मार्च</month>
							<month type="4">अप्रैल</month>
							<month type="5">मई</month>
							<month type="6">जून</month>
							<month type="7">जुलाई</month>
							<month type="8">अगस्त</month>
							<month type="9">सितम्बर</month>
							<month type="10">अक्टूबर</month>
							<month type="11">नवंबर</month>
							<month type="12">दिसंबर</month>
						</monthWidth>
					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="wide">
							<day type="sun">रविवार</day>
							<day type="mon">सोमवार</day>
							<day type="tue">मंगलवार</day>
							<day type="wed">बुधवार</day>
							<day type="thu">गुरुवार</day>
							<day type="fri">शुक्रवार</day>
							<day type="sat">शनिवार</day>
						</dayWidth>
					</dayContext>
				</days>
				<dayPeriods>
					<dayPeriodContext type="format">
						<dayPeriodWidth type="wide">
							<dayPeriod type="am">सबेरे</dayPeriod>
							<dayPeriod type="pm">सांम</dayPeriod>
						</dayPeriodWidth>
					</dayPeriodContext>
				</dayPeriods>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern draft="contributed">h:mm:ss a zzzz</pattern>
							<datetimeSkeleton draft="contributed">ahmmsszzzz</datetimeSkeleton>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern draft="contributed">h:mm:ss a z</pattern>
							<datetimeSkeleton draft="contributed">ahmmssz</datetimeSkeleton>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="medium">
						<timeFormat>
							<pattern draft="contributed">h:mm:ss a</pattern>
							<datetimeSkeleton draft="contributed">ahmmss</datetimeSkeleton>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="short">
						<timeFormat>
							<pattern draft="contributed">h:mm a</pattern>
							<datetimeSkeleton draft="contributed">ahmm</datetimeSkeleton>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
			</calendar>
		</calendars>
		<fields>
			<field type="year">
				<relative type="-1">गत बरस</relative>
				<relative type="0">ई बरस</relative>
				<relative type="1">आग्ल बरस</relative>
			</field>
			<field type="month">
				<relative type="-1">पाछलो मीनो</relative>
				<relative type="0">यों मीनो</relative>
				<relative type="1">आग्लो मीनो</relative>
			</field>
			<field type="week">
				<relative type="-1">पाछलां सप्ताह</relative>
				<relative type="0">ई सप्ताह</relative>
				<relative type="1">आग्ल सप्ताह</relative>
			</field>
			<field type="day">
				<relative type="-1">काल</relative>
				<relative type="0">आज</relative>
				<relative type="1">काल</relative>
			</field>
			<field type="sun">
				<relative type="-1">पाछलां दीतवार</relative>
				<relative type="0">ई दीतवार</relative>
				<relative type="1">आग्ल दीतवार</relative>
			</field>
		</fields>
		<timeZoneNames>
			<metazone type="GMT">
				<long>
					<standard>ग्रीन विच मीन टाइम</standard>
				</long>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<defaultNumberingSystem>deva</defaultNumberingSystem>
	</numbers>
</ldml>
