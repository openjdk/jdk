<ldml>
	<identity>
		<version number="$Revision$"/>
		<language type="vmw"/>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="en" draft="contributed">ingles</language>
			<language type="vmw" draft="contributed">emakhuwa</language>
		</languages>
		<scripts>
			<script type="Latn" draft="contributed">latim</script>
		</scripts>
		<territories>
			<territory type="MZ" draft="contributed">oMosambikhi</territory>
		</territories>
		<measurementSystemNames>
			<measurementSystemName type="metric" draft="contributed">metrikhu</measurementSystemName>
			<measurementSystemName type="UK" draft="contributed">RU</measurementSystemName>
			<measurementSystemName type="US" draft="contributed">EUA</measurementSystemName>
		</measurementSystemNames>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a c e f h i j k {kh} l m n o p {ph} r s t {th} {tt} {tth} u v w x y z]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[b d g q]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[\- ‐‑ – — , ; \: ! ? . … '‘’ &quot;“” ( ) \[ \] @ / \&amp; # ′ ″]</exemplarCharacters>
	</characters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="wide">
							<month type="1" draft="contributed">janeiru</month>
							<month type="2" draft="contributed">fevereiru</month>
							<month type="3" draft="contributed">marsu</month>
							<month type="4" draft="contributed">abril</month>
							<month type="5" draft="contributed">maiu</month>
							<month type="6" draft="contributed">junyu</month>
							<month type="7" draft="contributed">julyu</month>
							<month type="8" draft="contributed">agostu</month>
							<month type="9" draft="contributed">setembru</month>
							<month type="10" draft="contributed">outubru</month>
							<month type="11" draft="contributed">novembru</month>
							<month type="12" draft="contributed">dezembru</month>
						</monthWidth>
					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="wide">
							<day type="sun" draft="contributed">ettiminku</day>
							<day type="mon" draft="contributed">nihiku noolempwa</day>
							<day type="tue" draft="contributed">namaanli</day>
							<day type="wed" draft="contributed">namararu</day>
							<day type="thu" draft="contributed">namaxexe</day>
							<day type="fri" draft="contributed">namathanu</day>
							<day type="sat" draft="contributed">esaabadu</day>
						</dayWidth>
					</dayContext>
				</days>
				<dayPeriods>
					<dayPeriodContext type="format">
						<dayPeriodWidth type="wide">
							<dayPeriod type="am" draft="contributed">vaasisu</dayPeriod>
							<dayPeriod type="pm" draft="contributed">makaaripi</dayPeriod>
						</dayPeriodWidth>
					</dayPeriodContext>
				</dayPeriods>
				<dateTimeFormats>
					<availableFormats>
						<dateFormatItem id="yMd" draft="contributed">d/M/y</dateFormatItem>
						<dateFormatItem id="yMMMd" draft="contributed">d MMM y</dateFormatItem>
					</availableFormats>
					<intervalFormats>
						<intervalFormatFallback draft="contributed">{0} ‘mpakha’ {1}</intervalFormatFallback>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
		<timeZoneNames>
			<hourFormat draft="contributed">+H:mm;-H:mm</hourFormat>
			<regionFormat draft="contributed">okathi wa {0}</regionFormat>
			<regionFormat type="daylight" draft="contributed">okathi wa othana wa {0}</regionFormat>
			<regionFormat type="standard" draft="contributed">okathi oolikana wa {0}</regionFormat>
			<metazone type="GMT">
				<long>
					<standard draft="contributed">okathi oolikana wa Greenwich</standard>
				</long>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<symbols numberSystem="latn">
			<decimal draft="contributed">,</decimal>
			<group draft="contributed">.</group>
		</symbols>
		<currencyFormats numberSystem="latn">
			<currencyFormatLength>
				<currencyFormat type="standard">
					<pattern draft="contributed">#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>
