    FILES := $(wildcard $(addprefix $(BUILD_HOTSPOT_JTREG_OUTPUT_DIR), /bin/* /lib/*)), \
    FLATTEN := true, \
))

test-image-hotspot-jtreg-native: $(COPY_HOTSPOT_JTREG_NATIVE)

all: build-test-hotspot-jtreg-native
