instruct vshift32B_avx(vec dst, vec src, vec shift, vec tmp) %{
  predicate(Matcher::vector_length(n) == 32 && !n->as_ShiftV()->is_var_shift());
  match(Set dst ( LShiftVB src shift));
  match(Set dst ( RShiftVB src shift));
