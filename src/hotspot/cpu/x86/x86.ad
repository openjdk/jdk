            "setcc   $dst \t# emits setne + movzbl or setzune for APX"
    "done:" %}
  ins_encode %{
    Label done;
    __ cmpq($src1$$Register, $src2$$Register);
    __ movl($dst$$Register, -1);
    __ jccb(Assembler::below, done);
    __ setcc(Assembler::notZero, $dst$$Register);
    __ bind(done);
  %}
  ins_pipe(pipe_slow);
%}

