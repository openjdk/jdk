  format %{ "vector_rearrange $dst, $shuffle, $src!\t using $xtmp1, $xtmp2, $xtmp3, $rtmp and $ktmp as TEMP" %}
  ins_encode %{
    int vlen_enc = vector_length_encoding(this);
