  match(Set dst (AddL dst src));
  effect(KILL cr);
  flag(PD::Flag_sets_overflow_flag, PD::Flag_sets_sign_flag, PD::Flag_sets_zero_flag, PD::Flag_sets_carry_flag, PD::Flag_sets_parity_flag);

  format %{ "addq    $dst, $src\t# long" %}
